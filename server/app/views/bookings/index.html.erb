
<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Seat</th>
      <th>Event</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.seat_id %></td>
        <td><%= booking.event.name %></td>
        <td><%= booking.user.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
